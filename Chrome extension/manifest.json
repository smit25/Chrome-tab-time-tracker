{
	"manifest_version": 2,
	"name": "Geturl",
	"description": "Fetches current tab url.",
	"version": "1.0",
	"author": "Smit Patel",
	"browser_action": {
		"default_icon": "computer.png",
		"default_title": "Just observing your current url."
	},
	"permissions": ["tabs", "activeTab", "http://127.0.0.1:5000/*", "storage","webRequest", "webRequestBlocking"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}